<bean id="Payroll" class="org.apache.activemq.ActiveMQConnectionFactory">
     <property name="userName" value="${AMQConnection.user}"/>
     <property name="password" value="${AMQConnection.password}"/>
     <property name="brokerURL" value="${AMQConnection.url}" />
</bean>
<!-- Payroll MQ Details End -->

<!-- Logistics MQ Details Start -->
<!-- <bean id="Logistics" class="org.apache.camel.component.jms.JmsComponent"><property name="connectionFactory" ref="LogisticsCreds" /></bean>
<bean id="LogisticsCreds" class="org.springframework.jms.connection.UserCredentialsConnectionFactoryAdapter"><property name="targetConnectionFactory" ref="LogisticsFactory" /><property name="username" value="admin" /><property name="password" value="Miracle@1234" /></bean>
<bean id="LogisticsFactory" class="com.ibm.mq.jms.MQQueueConnectionFactory"><property name="hostName" value="mq-ibm-mq" /><property name="port" value="1414" /><property name="queueManager" value="mq" /><property name="channel" value="DEV.ADMIN.SVRCONN" /><property name="transportType" value="1" /></bean> -->
<bean id="Logistics" class="org.springframework.jms.connection.UserCredentialsConnectionFactoryAdapter">
     <property name="targetConnectionFactory" ref="myConnectionFactory" />
     <property name="username" value="${MQConnection.User}" />
     <property name="password" value="${MQConnection.Pwd}" />
</bean>
<!-- Logistics MQ Details End -->

<!-- Application MQ Details End -->


<!-- Application New MQ Details Start -->

<!-- CustomerInteract MQ Details Start -->
<bean id="CustomerInteract" class="org.apache.activemq.ActiveMQConnectionFactory">
     <property name="userName" value="${AMQConnection.user}"/>
     <property name="password" value="${AMQConnection.password}"/>
     <property name="brokerURL" value="${AMQConnection.url}" />
</bean>
<!-- CustomerInteract MQ Details End -->

<!-- Sourcing MQ Details Start -->
<bean id="Sourcing" class="org.springframework.jms.connection.UserCredentialsConnectionFactoryAdapter">
     <property name="targetConnectionFactory" ref="myConnectionFactory" />
     <property name="username" value="${MQConnection.User}" />
     <property name="password" value="${MQConnection.Pwd}" />
</bean>
<!-- Sourcing MQ Details End -->

<!-- Application New MQ Details End -->

</beans>